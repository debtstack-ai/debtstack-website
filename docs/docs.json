{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "DebtStack",
  "logo": {
    "dark": "/logo/logo-dark.png",
    "light": "/logo/logo-light.png",
    "href": "https://debtstack.ai"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#3B82F6",
    "light": "#60A5FA",
    "dark": "#1D4ED8"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "rounded": "default",
  "background": {
    "style": "gradient"
  },
  "codeBlock": {
    "mode": "dark"
  },
  "search": {
    "prompt": "Search docs..."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "topbarLinks": [
    {
      "name": "Dashboard",
      "url": "https://debtstack.ai/dashboard"
    }
  ],
  "topbarCtaButton": {
    "name": "Get API Key",
    "url": "https://debtstack.ai/dashboard"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "concepts/data-model",
              "concepts/field-selection",
              "concepts/pagination"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/scenarios",
              "guides/ai-agents",
              "guides/langchain",
              "guides/mcp"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/overview"
            ]
          },
          {
            "group": "Companies",
            "pages": [
              "api-reference/companies/search"
            ]
          },
          {
            "group": "Bonds",
            "pages": [
              "api-reference/bonds/search",
              "api-reference/bonds/resolve",
              "api-reference/bonds/pricing"
            ]
          },
          {
            "group": "Financials",
            "pages": [
              "api-reference/financials/search"
            ]
          },
          {
            "group": "Collateral",
            "pages": [
              "api-reference/collateral/search"
            ]
          },
          {
            "group": "Entities",
            "pages": [
              "api-reference/entities/traverse"
            ]
          },
          {
            "group": "Documents",
            "pages": [
              "api-reference/documents/search"
            ]
          },
          {
            "group": "Covenants",
            "pages": [
              "api-reference/covenants/search",
              "api-reference/covenants/compare"
            ]
          },
          {
            "group": "Batch",
            "pages": [
              "api-reference/batch/operations"
            ]
          }
        ]
      }
    ]
  },
  "footerSocials": {
    "twitter": "https://twitter.com/debtstack",
    "github": "https://github.com/debtstack",
    "linkedin": "https://linkedin.com/company/debtstack"
  },
  "api": {
    "baseUrl": "https://api.debtstack.ai",
    "auth": {
      "method": "key",
      "name": "X-API-Key"
    }
  }
}
